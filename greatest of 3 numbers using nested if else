a=int(input("enter the a value"))
b=int(input("enter the b value"))
c=int(input("enter the c value"))
if(a>b):
    if(a>c):
        print("the greatest no is",a)
    else:
        print("the greatest no is",c)
else:
    if(b>c):
        print("the greatest no is",b)
    else:
        print("the greatest no is",c)
